{
  "title": "Study",
  "type": "object",
  "properties": {
    "studyTitle": {
      "title": "Title",
      "type": "string",
      "description": "Study Title"
    },
    "studyDescription": {
      "title": "Study Description",
      "type": "string"
    },
    "subjects": {
      "title": "Subjects",
      "description": "Subjects involved in this study",
      "type": "array",
      "items": {
        "type": "object",
        "title": "Subject",
        "properties": {
          "UUID": {
            "type": "string",
            "title": "Subject UUID",
            "description": "Subject unique identifier ID. Generated by the repository, not user input"
          },
          "group": {
            "title": "Subject Group",
            "type": "string",
            "description": "Study group that this subject belongs to (control, experimental, healthy, etc.)"
          },
          "gender": {
            "title": "Gender",
            "type": "string"
          },
          "yob": {
            "type": "number",
            "title": "Year of Birth"
          },
          "height": {
            "title": "Height",
            "type": "number"
          },
          "weight": {
            "title": "Weight",
            "type": "number"
          },
          "handedness": {
            "title": "Handedness",
            "type": "string",
            "enum": ["Left", "Right", "Both"]
          },
          "vision": {
            "title": "Vision",
            "type": "string",
            "enum": [
              "Normal",
              "Corrected to normal",
              "Impaired",
              "NA",
              "-",
              " "
            ]
          },
          "hearing": {
            "title": "Hearing",
            "type": "string",
            "enum": [
              "Normal",
              "Corrected to normal",
              "Impaired",
              "NA",
              "-",
              " "
            ]
          },
          "additionalInfo": {
            "title": "Additional Information",
            "type": "string",
            "description": "Additional subject information or link to psychological/physiological evaluations, etc."
          },
          "channelLocations": {
            "title": "Channel Locations",
            "type": "string",
            "description": "Link to subject electrode location file in any format"
          },
          "label": {
            "title": "Subject ID",
            "type": "string",
            "description": "User-generated label for this Subject object."
          }
        }
      }
    },
    "stimuli": {
      "title": "Stimuli",
      "description": "Stimulus presented to a subject in the study",
      "type": "array",
      "items": {
        "type": "object",
        "title": "Stimulus",
        "properties": {
          "fileLocation": {
            "title": "Stimulus File",
            "type": "string",
            "description": "Link to the stimulus file"
          },
          "stimulusType": {
            "title": "Stimulus Type",
            "type": "string",
            "description": "The type/modality of the stimulus presented",
            "enum": ["Audio", "Image", "Video", "Other"]
          },
          "stimulusDescription": {
            "title": "Stimulus Description",
            "type": "string",
            "description": "Optional additional description of the stimulus"
          },
          "UUID": {
            "title": "Stimulus UUID",
            "type": "string",
            "description": "Unique identifier of this Stimulus object, generated by repository"
          },
          "label": {
            "title": "Stimulus Label",
            "type": "string",
            "description": "User-generated label for this Stimulus object, if not provided defaults to UUID"
          }
        }
      }
    },
    "recordingParameterSets": {
      "title": "Recording Parameter Sets",
      "description": "Set of parameters describing the EEG recording setup used in a particular recording",
      "type": "array",
      "items": {
        "type": "object",
        "title": "RecordingParameterSet",
        "properties": {
          "samplingRate": {
            "title": "Sampling Rate",
            "type": "number"
          },
          "startChannel": {
            "title": "Start Channel",
            "type": "number"
          },
          "endChannel": {
            "title": "End Channel",
            "type": "number"
          },
          "referenceChannels": {
            "title": "Reference Channels",
            "type": "array",
            "items": {
              "type": "number",
              "title": "Reference Channel Number"
            }
          },
          "nonScalpChannels": {
            "title": "Non-Scalp Channels",
            "type": "array",
            "items": {
              "type": "number",
              "title": "Non-scalp Channel Number"
            }
          },
          "label": {
            "title": "Label",
            "type": "string",
            "description": "User-generated label for this RecordingParameterSet object, if not provided defaults to UUID"
          },
          "UUID": {
            "title": "Recording Parameters ID",
            "type": "string",
            "description": "Unique recording parameter set UUID, generated by repository"
          }
        }
      }
    },
    "recordings": {
      "title": "Recordings",
      "description": "Single recording session file. May include multiple events, but only a single subject.",
      "type": "array",
      "items": {
        "type": "object",
        "title": "Recording",
        "properties": {
          "fileLocation": {
            "title": "Recording File",
            "type": "string",
            "description": "Link to the recording file location"
          },
          "eventUUIDs": {
            "title": "Event IDs",
            "type": "array",
            "items": {
              "type": "string",
              "title": "Event ID"
            }
          },
          "subjectUUID": {
            "title": "Subject ID",
            "type": "string"
          },
          "recordingParameterSetUUID": {
            "title": "Recording Parameters ID",
            "type": "string"
          },
          "UUID": {
            "title": "Recording UUID",
            "type": "string",
            "description": "UUID of this Recording object"
          },
          "label": {
            "title": "Recording Label",
            "type": "string",
            "description": "User-generated label for this Recording object, if not provided defaults to UUID"
          }
        }
      }
    },
    "events": {
      "title": "Events",
      "description": "A single event that defines an epoch of EEG data. Must have a starting trigger/event value, and may have an optional end value",
      "type": "array",
      "items": {
        "type": "object",
        "title": "Event",
        "properties": {
          "stimulusUUID": {
            "title": "Stimulus UUID",
            "type": "string",
            "description": "UUID of the stimulus corresponding to this event"
          },
          "startTrigger": {
            "title": "Start Trigger",
            "type": "number"
          },
          "endTrigger": {
            "title": "End Trigger",
            "type": "number",
            "description": "Optional ending trigger value"
          },
          "UUID": {
            "title": "Event UUID",
            "type": "string",
            "description": "UUID for this Event object, generated by the repository"
          },
          "eventDescription": {
            "title": "Event Description",
            "type": "string",
            "description": "Optional additional description of the Event"
          },
          "label": {
            "title": "Event Label",
            "type": "string",
            "description": "User-generated label for this Event object, if not provided defaults to UUID"
          }
        }
      }
    },
    "publications": {
      "title": "Publications",
      "description": "Publications this study has been involved in",
      "type": "array",
      "items": {
        "type": "object",
        "title": "Publication",
        "properties": {
          "citation": {
            "title": "Citation",
            "type": "string",
            "description": "Citation in any format, BibTex parsed automatically"
          },
          "link": {
            "title": "Link",
            "type": "string",
            "description": "Link to the publication"
          },
          "UUID": {
            "title": "Publication UUID",
            "type": "string",
            "description": "UUID of this Publication object"
          },
          "label": {
            "title": "Publication Label",
            "type": "string",
            "description": "User-generated label for this Publication object, if not provided defaults to UUID"
          }
        }
      }
    },
    "experimenters": {
      "title": "Experimenters",
      "description": "Experimenters involved in the study",
      "type": "array",
      "items": {
        "type": "object",
        "title": "Experimenter",
        "properties": {
          "firstName": {
            "title": "First Name",
            "type": "string"
          },
          "lastName": {
            "title": "Last Name",
            "type": "string"
          },
          "role": {
            "title": "Role",
            "type": "string"
          },
          "affiliation": {
            "title": "Affiliation",
            "type": "string",
            "description": "Institutional affiliation of the experimenter"
          },
          "UUID": {
            "title": "",
            "type": "string",
            "description": "UUID of this Experimenter object"
          },
          "label": {
            "title": "Event Label",
            "type": "string",
            "description": "User-generated label for this Experimenter object, if not provided defaults to firstName+' '+lastName"
          }
        }
      }
    },
    "license": {
      "title": "License",
      "description": "Describes the licensing of the study",
      "type": "object",
      "properties": {
        "licenseType": {
          "type": "string",
          "description": "License type"
        },
        "licenseLocation": {
          "type": "string",
          "description": "License file"
        }
      }
    },
    "contacts": {
      "description": "Author contact information",
      "type": "array",
      "title": "Contacts",
      "items": {
        "type": "object",
        "title": "Contact",
        "properties": {
          "firstName": {
            "type": "string"
          },
          "lastName": {
            "type": "string"
          },
          "phoneNumber": {
            "type": "number",
            "maxLength": 10
          },
          "email": {
            "type": "string"
          },
          "UUID": {
            "type": "string",
            "title": "Contact UUID",
            "description": "UUID of this Contact object"
          },
          "label": {
            "title": "Contact Label",
            "type": "string",
            "description": "User-generated label for this Contact object, if not provided defaults to firstName+' '+lastName"
          }
        }
      }
    }
  }
}
